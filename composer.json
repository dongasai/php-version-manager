{
    "name": "dongasai/php-version-manager",
    "description": "A simple PHP CLI version management tool.",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "symfony/console": "^4.0",
        "symfony/process": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "VersionManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["bin/pvm"],
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.pages.dev"
        }
    }
}
